# 🌟 Configuration Blunof - Variables d'environnement
# Copiez ce fichier vers .env et configurez vos valeurs

# 🚀 Application
VITE_APP_NAME=Blunof
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# 📱 Telegram
VITE_TELEGRAM_BOT_TOKEN=your_bot_token_here
VITE_TELEGRAM_BOT_USERNAME=blunof_bot
VITE_TELEGRAM_WEBAPP_URL=https://your-domain.com

# 🌐 API & Services
VITE_API_BASE_URL=https://api.blunof.com
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_key
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id

# 📊 Analytics
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# 🔒 Sécurité
VITE_ENCRYPTION_KEY=your_encryption_key_here
VITE_JWT_SECRET=your_jwt_secret_here

# 📁 Stockage
VITE_CLOUDFLARE_R2_BUCKET=your_bucket_name
VITE_CLOUDFLARE_R2_ENDPOINT=https://your_account_id.r2.cloudflarestorage.com
VITE_CLOUDFLARE_R2_ACCESS_KEY_ID=your_access_key_id
VITE_CLOUDFLARE_R2_SECRET_ACCESS_KEY=your_secret_access_key

# 🎨 Design
VITE_PRIMARY_COLOR=#2196F3
VITE_SECONDARY_COLOR=#000000
VITE_ACCENT_COLOR=#FFD700

# 📧 Email
VITE_SMTP_HOST=smtp.gmail.com
VITE_SMTP_PORT=587
VITE_SMTP_USER=your_email@gmail.com
VITE_SMTP_PASS=your_app_password

# 🔔 Notifications
VITE_PUSH_PUBLIC_KEY=your_push_public_key
VITE_WEBHOOK_URL=https://your-domain.com/webhook

# 🧪 Tests
VITE_TEST_MODE=false
VITE_MOCK_DATA=true
VITE_DEBUG_LEVEL=info
